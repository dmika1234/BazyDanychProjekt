require("OpenRepGrid")
require(dplyr)
require(randomNames)
require("RPostgres")
require(data.table)
email_platforms <- c("@gmail.com", "@onet.pl", "@wp.pl", "@o2.pl", "@yahoo.com")
znaki <- 1:61
znaki[1:26] <- letters
znaki[27:52] <- LETTERS
znaki[53:61] <- 1:9
znaki <- as.character(znaki)
##
sink(file = "konta.txt")
mails <- 1:2000
for(i in 1:2000){
email <- paste(randomWords(1),randomWords(1), sample(znaki, 1), sample(email_platforms, 1, prob = c(0.6, 0.13, 0.13, 0.13, 0.01)), sep = "")
while(email == unique(mail))
email <- paste(randomWords(1),randomWords(1), sample(znaki, 1), sample(email_platforms, 1), sep = "")
mail[i] <- email
il_haslo <- sample(8:16)
haslo <- paste(sample(znaki, il_haslo, replace = TRUE), collapse = "")
data <- sample(seq(as.Date('2013/01/01'), as.Date('2021/01/10'), by="day"), 1)
id_planu <- sample(1:5, 1)
cat(paste0("INSERT INTO konta(email, haslo, data_zalozenia, id_planu) VALUES ('", email, "','",
haslo, "','", data, "','", id_planu, "');", "\n"))
}
sink()
